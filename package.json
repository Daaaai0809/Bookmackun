{
  "name": "@bookmackun/bot",
  "type": "module",
  "types": "./dist/index.d.ts",
  "main": "./dist/index.js",
  "scripts": {
    "dev": "wrangler dev src/index.ts",
    "deploy": "wrangler deploy --minify src/index.ts",
    "build": "tsc -p tsconfig.json",
    "generate:wrangler": "twrangler build",
    "generate:migrations": "drizzle-kit generate:sqlite --out ./drizzle/migrations --schema src/drizzle/schema.ts"
  },
  "dependencies": {
    "better-sqlite3": "^9.5.0",
    "discord-api-types": "^0.37.79",
    "discord-interactions": "^3.4.0",
    "drizzle-orm": "^0.30.8",
    "hono": "^4.2.3",
    "twrangler": "^0.0.7"
  },
  "devDependencies": {
    "@biomejs/biome": "1.7.0",
    "@cloudflare/workers-types": "^4.20240403.0",
    "drizzle-kit": "^0.20.14",
    "wrangler": "^3.47.0"
  }
}
