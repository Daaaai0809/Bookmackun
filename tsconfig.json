{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "outDir": "./dist",
    "strict": true,
    "lib": [
      "ESNext",
      "dom"
    ],
    "types": [
      "@cloudflare/workers-types"
    ],
    "paths": {
      "@/*": ["./src/*"],
    },
    "jsx": "react-jsx",
    "jsxImportSource": "hono/jsx",
  },
}
